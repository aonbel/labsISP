<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:UI.Pages"
    FlyoutBehavior="Flyout"
    Title="UI">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPageModel}"
        Route="MainPage" />
    
    <ShellContent
        Title="Artists list"
        ContentTemplate="{DataTemplate pages:ArtistsPageModel}"
        Route="Artists" />

</Shell>
